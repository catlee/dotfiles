alias vi=nvim
alias vim=nvim
alias dt="dev test --include-branch-commits"
alias drt="dev retest"
alias ds="dev style --include-branch-commits"
alias dc="dev typecheck"
alias da="dt && ds && dc"
alias dsc="ds && dc"
alias dsc="ds && dc"
alias tnew="tmux new -As0"
alias gpa="git pull --rebase && update"
alias spinname='spin show -o fqdn'
alias bx="bundle exec"
